	<section itemscope="" itemtype="https://schema.org/Person" class="message-user ">
		<div class="message-avatar ">
			<div class="message-avatar-wrapper">
				
					<a href="https://xdaforums.com/m/dragon-kumera.9795230/" class="avatar avatar--s avatar--default avatar--default--dynamic" data-user-id="9795230" data-xf-init="member-tooltip" style="background-color: #9e9d24; color: #f4ff81" id="js-XFUniqueId29">
